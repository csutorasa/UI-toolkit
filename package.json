{
  "name": "ui-toolkit",
  "version": "0.0.0",
  "description": "UI-toolkit",
  "main": "server.js",
  "scripts": {
    "install": "npm run clean && npm run typescript && npm run sass",
    "start": "node server.js",
    "test": "echo \"No tests are specified\"",
    "watch": "concurrently --prefix \"[{name}]\" --names \"TypeScript,Sass\" \"npm run typescript-watch\" \"npm run sass-watch\"",
    "clean": "rimraf target",
    "typescript": "tsc --project .",
    "typescript-watch": "tsc --watch --project .",
    "sass": "node-sass --output-style compressed uitoolkit/main.scss target/main.css",
    "sass-watch": "node-sass --watch --output-style compressed uitoolkit/main.scss target/main.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/csutorasa/UI-toolkit.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/csutorasa/UI-toolkit/issues"
  },
  "homepage": "https://github.com/csutorasa/UI-toolkit#readme",
  "dependencies": {
    "@angular/common": "^2.4.9",
    "@angular/compiler": "^2.4.9",
    "@angular/core": "^2.4.9",
    "@angular/forms": "^2.4.9",
    "@angular/http": "^2.4.9",
    "@angular/platform-browser": "^2.4.9",
    "@angular/platform-browser-dynamic": "^2.4.9",
    "@angular/router": "^3.4.9",
    "@types/es6-shim": "^0.31.32",
    "@types/node": "^7.0.8",
    "core-js": "^2.4.1",
    "node-sass": "^4.5.0",
    "reflect-metadata": "^0.1.10",
    "rimraf": "^2.6.1",
    "rxjs": "^5.2.0",
    "systemjs": "^0.19.46",
    "typescript": "^2.2.1",
    "uglify": "^0.1.5",
    "zone.js": "^0.7.8"
  },
  "devDependencies": {
    "concurrently": "^3.4.0"
  }
}
