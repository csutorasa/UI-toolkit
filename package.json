{
  "name": "ui-toolkit-showcase",
  "version": "1.0.0",
  "description": "UI toolkit Showcase",
  "main": "server.js",
  "scripts": {
    "install": "cd uitoolkit && npm install && cd .. && npm run clean && npm run typescript && npm run webpack",
    "start": "node server.js",
    "test": "echo \"No tests are specified\"",
    "watch": "concurrently --prefix \"[{name}]\" --names \"TypeScript,Sass\" \"npm run typescript-watch\" \"npm run sass-watch\"",
    "clean": "rimraf upload",
    "webpack": "webpack --config webpack.config.js",
    "typescript": "tsc --project ./server",
    "typescript-watch": "tsc --watch --project ./server",
    "sass": "node-sass --output-style compressed uitoolkit/main.scss target/main.css",
    "sass-watch": "node-sass --watch --output-style compressed uitoolkit/main.scss target/main.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/csutorasa/UI-toolkit.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/csutorasa/UI-toolkit/issues"
  },
  "homepage": "https://github.com/csutorasa/UI-toolkit#readme",
  "dependencies": {
    "@angular/common": "^4.0.1",
    "@angular/compiler": "^4.0.1",
    "@angular/core": "^4.0.1",
    "@angular/forms": "^4.0.1",
    "@angular/http": "^4.0.1",
    "@angular/platform-browser": "^4.0.1",
    "@angular/platform-browser-dynamic": "^4.0.1",
    "@angular/router": "^4.0.1",
    "@types/es6-shim": "^0.31.32",
    "@types/node": "^7.0.12",
    "@types/reflect-metadata": "0.0.5",
    "@types/webpack": "^2.2.14",
    "bootstrap": "^4.0.0-alpha.6",
    "core-js": "^2.4.1",
    "node-sass": "^4.5.2",
    "reflect-metadata": "^0.1.10",
    "rimraf": "^2.6.1",
    "rxjs": "^5.3.0",
    "systemjs": "^0.19.46",
    "ts-loader": "^2.0.3",
    "typescript": "^2.2.2",
    "webpack": "^2.3.3",
    "zone.js": "^0.8.5"
  },
  "devDependencies": {
    "concurrently": "^3.4.0"
  }
}
