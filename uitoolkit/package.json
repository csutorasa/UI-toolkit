{
  "name": "ui-toolkit",
  "version": "1.0.0",
  "description": "UI-toolkit",
  "main": "server.js",
  "scripts": {
    "install": "npm run clean && npm run webpack && npm run sass",
    "start": "echo \"Nothing to start\"",
    "test": "echo \"No tests are specified\"",
    "watch": "concurrently --prefix \"[{name}]\" --names \"TypeScript,Sass\" \"npm run typescript-watch\" \"npm run sass-watch\"",
    "clean": "rimraf bundles",
    "webpack": "webpack --config webpackdev.config.js && webpack --config webpackprod.config.js",
    "typescript": "tsc --project .",
    "typescript-watch": "tsc --watch --project .",
    "sass": "node-sass --output-style compressed index.scss bundles/uitoolkit.css",
    "sass-watch": "node-sass --watch --output-style compressed main.scss bundles/uitoolkit.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/csutorasa/UI-toolkit.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/csutorasa/UI-toolkit/issues"
  },
  "homepage": "https://github.com/csutorasa/UI-toolkit#readme",
  "peerDependencies": {
    "bootstrap": "^4.0.0-alpha.6",
    "@angular/common": "^4.0.1",
    "@angular/compiler": "^4.0.1",
    "@angular/core": "^4.0.1",
    "@angular/forms": "^4.0.1",
    "@angular/http": "^4.0.1",
    "@angular/platform-browser": "^4.0.1",
    "@angular/platform-browser-dynamic": "^4.0.1",
    "@angular/router": "^4.0.1",
    "rxjs": "^5.3.0",
    "zone.js": "^0.8.5"
  },
  "dependencies": {
    "@types/es6-shim": "^0.31.32",
    "@types/node": "^7.0.12",
    "node-sass": "^4.5.2",
    "reflect-metadata": "^0.1.10",
    "rimraf": "^2.6.1",
    "systemjs": "^0.19.46",
    "ts-loader": "^2.0.3",
    "typescript": "^2.2.2",
    "webpack": "^2.3.3"
  },
  "devDependencies": {
    "concurrently": "^3.4.0"
  }
}
