{
  "name": "wizyx",
  "version": "1.0.0",
  "description": "UI-toolkit",
  "main": "server.js",
  "scripts": {
    "install": "npm run clean && npm run buildjs && npm run buildcss",
    "start": "echo \"Nothing to start\"",
    "test": "echo \"No tests are specified\"",
    "watch": "concurrently --prefix \"[{name}]\" --names \"WebpackDev,WebpackMin,CSS\" \"npm run watchdevjs\" \"npm run watchprodjs\" \"npm run watchcss\"",
    "clean": "rimraf bundles && rimraf src/**/*.d.ts",
    "buildjs": "webpack --config webpackdev.config.js && webpack --config webpackprod.config.js",
    "watchdevjs": "webpack --config webpackdev.config.js --watch",
    "watchprodjs": "webpack --config webpackprod.config.js --watch",
    "buildcss": "node-sass --output-style compressed index.scss bundles/uitoolkit.css",
    "watchcss": "node-sass --watch --output-style compressed index.scss bundles/uitoolkit.css",
    "typescript": "tsc --project ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/csutorasa/UI-toolkit.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/csutorasa/UI-toolkit/issues"
  },
  "homepage": "https://github.com/csutorasa/UI-toolkit#readme",
  "peerDependencies": {
    "bootstrap": "^4.0.0-alpha.6",
    "@angular/common": "^4.0.1",
    "@angular/compiler": "^4.0.1",
    "@angular/core": "^4.0.1",
    "@angular/forms": "^4.0.1",
    "@angular/http": "^4.0.1",
    "@angular/platform-browser": "^4.0.1",
    "@angular/platform-browser-dynamic": "^4.0.1",
    "@angular/router": "^4.0.1",
    "rxjs": "^5.3.0",
    "zone.js": "^0.8.5"
  },
  "dependencies": {
    "@types/node": "^7.0.12",
    "node-sass": "^4.5.2",
    "reflect-metadata": "^0.1.10",
    "rimraf": "^2.6.1",
    "ts-loader": "^2.0.3",
    "typescript": "^2.2.2",
    "webpack": "^2.3.3"
  },
  "devDependencies": {
    "concurrently": "^3.4.0"
  }
}
